@charset "UTF-8";

/* fonts */

@font-face {
    font-family: 'blocks-font';
    src: url('../fonts/BLOKKNeue-Regular.eot');
    src: url('../fonts/BLOKKNeue-Regular.eot?#iefix') format('embedded-opentype'),
         url('../fonts/BLOKKNeue-Regular.woff') format('woff'),
         url('../fonts/BLOKKNeue-Regular.svg#BLOKKNeue-Regular') format('svg');
    font-weight: normal;
    font-style: normal;
}

/* Body */
html, body {
	margin: 0;
	padding: 0;
	height: 100%;
	width: 100%;	
	-webkit-font-smoothing: antialiased;
}
/* Header */
.fixed .header,
.fixed .section-header {
	position: fixed;	
}

.header {
	width: 100%;
	background: #202326;
	height: 70px;
	top: 0px;
	left: 0px;
	z-index: 2;
	font-family: 'Lato', sans-serif;
	// position: absolute;
}
.caption-learn {
	display: block;
	text-align: center;
	margin-top: 20px;
}
.learn-more {
	display: inline-block;
	border: 2px solid;
	width: 10%;
	font-size: 14px;
    font-weight: bolder;
    padding: 10px;
}
.caption {
	z-index: 3;
	position: absolute;
	width: 100%;
	text-align: center;
	color: rgb(255, 255, 255);
	font-family: Raleway, Arial, sans-serif;
	text-transform: uppercase;
	font-size: 24px;
	background: -webkit-linear-gradient(rgba(0, 0, 0, 0), rgba(47,44,44,1));
	top: 130px;
	left: 0px;
	right: 0px;
	bottom: 0;
	padding: 45px 0 0 0;
	height: 450px;
}
.sponsored {
	color: #fff;
	text-align: left;
	font-weight: normal;
	font-size: 12px;
}
.caption span {
	display: block;
}
.caption .headline {
	font-weight: bold;
	font-size: 55px;
}
.caption .description {
	font-size: 20px;
}
.search {
	overflow: hidden;
	height: 70px;
}
.search.clicked {
	overflow: visible;
}
.content-box.videos-template .player {
	visibility: hidden;
	background: #000;
}
.bookmarks .content-box.videos-template.selected .player {
	visibility: visible;
}
.bookmarks .content-box.videos-template .player {
	position: absolute;
	z-index: 4;
	height: 100%;
	width: 100%;
	top: 0px;
}
.search.clicked .arrow-up {
	-ms-transform: rotate(180deg); /* IE 9 */
    -webkit-transform: rotate(180deg); /* Chrome, Safari, Opera */
    transform: rotate(180deg);	
}
.search.clicked .menu {
	opacity: 1;
	overflow:visible;
}

.logo {
	background-image: url("<%= asset_path("White_Logo.png") %>");
	height: 85px;
	width: 300px;
	position: fixed;
	background-size: 75% auto;
	background-position: center 3px;
	background-repeat: no-repeat;
	z-index: 3;
	top: 0px;
	-webkit-transition: -webkit-transform 0.2s ease-in-out;
}
.fixed .logo {
	-webkit-transform: scale(0.8);
	top: -8px;
	background-position: center -75px;
}

.header .category, 
.header .date, 
.header .type {
	color: #fff;
	text-align: left;
	float: left;
	width: 190px;
	clear: none;
	padding: 0;
	margin: 0 5px;
	position: relative;
}
.search-fields .search:hover {
	background: #f1f1f1;
}
.search-fields .search:hover .header-text {
	color: #5b5b5b;
}
.search-fields .search:hover .arrow-up {
	border-bottom: 5px solid #202326;
}
.search-fields .search:hover .currently-selected {
	color: #404040;
}

.currently-selected {
	display: block;
	text-align: left;
	padding: 0 0 0 20px;
	text-transform: uppercase;
	color: #fff;
	font-family: 'Source Sans Pro', sans-serif;
	font-size: 13px;
}

.header-text {
	color: #bfbfbf;
	display: block;
	font-size: 11px;
	text-align: left;
	padding: 15px 0 5px 20px;
}

.content-box.ads-template {
	font-family: Raleway, Arial, sans-serif;
	line-height: 254px;
	font-weight: bold;
	text-transform: uppercase;
	color: #454545;
	background: #fff;
}
.content-box.ads-template a {
	color: #303030;
	text-decoration: none;
	top: 0px;
	display: block;
	text-align: center;
	position: absolute;
	width: 100%;
	left: 0px;
	bottom: 0px;
	right: 0px;
}
.load-more {
	font-family: Raleway, Arial, sans-serif;
	color: #aaaaaa;
	background: #202326;
	padding: 20px;
	margin: 25px 25px 0 25px;
	display: block;
	clear: both;
	cursor: pointer;
	text-align: center;
}
.bookmarks .load-more {
	margin: 25px 0;
	font-size: 21px;
	padding: 20px;
}
.load-more.loaded {
	cursor: default;	
}
.load-more:hover {
	background: #aaaaaa;
	color: #202326;
}
.load-more.loaded:hover {
	color: #aaaaaa;
	background: #202326;
}
.bookmarks .content-box.ads-template {
	line-height: normal;
	background: #fff;
}
.content-box.ads-template span {
	display: block;
	text-align: center;
	-webkit-transform: translate(-50%,-50%);
	top: 50%;
	left: 50%;
	position: absolute;
}

.arrow {
	color: #fff;
	font-size: 85px;
}

.selected-link {
	height: 70px;
	text-align: center;
	font-family: 'Source Sans Pro', sans-serif;
	text-transform: none;
	font-weight: bold;
	font-size: 16px;
	color: #fff;
	border-left: 1px solid #3e4145;
	border-right: 1px solid #3e4145;
}

.arrow-up {
	width: 0;
	border-left: 5px solid transparent;
	border-right: 5px solid transparent;
	border-bottom: 5px solid #FFFFFF;
	position: absolute;
	right: 15px;
	top: 43px;
}
.thumbnails {
	display: none;
	margin: 0 0 0 -10px;
	overflow: hidden;
}
.thumbnails li {
	width: 100px;
	height: 100px;
	float: left;
	margin: 5px;
	overflow: hidden;
	cursor: pointer;
	list-style: none;
	position: relative;
	border: solid 3px #000;
	background: #000;
}
.thumbnails li img {
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%,-50%);
	max-width: 100%;
	max-height: 100%;
	position: absolute;
}
.menu {
	background: #000;
	color: #fff;
	border-top: solid 1px #fff;
	font-weight: normal;
	opacity: 0;
	-webkit-transition: opacity 0.2s ease-in-out;
}

.menu li {
	list-style: none;
	margin: 15px 20px 0 20px;
	font-size: 14px;
	text-align: left;
	text-transform: none;
	border-bottom: solid 1px #fff;
	padding: 0 0 15px 0;
}

.menu li.selected,.menu li:hover {
	list-style-type: inherit;
	color: #45ad58;
}

.menu li:last-child {
	border: none;	
}

.search-fields {
	width: auto;
	cursor: pointer;
	margin: 0 0 0 300px;
	overflow: hidden;
}

/* Slideshow */
.slideshow {
	height: 270px;
	text-align: center;
	width: 100%;
	// position: fixed;
	z-index: -1;
	top: 0px;
	font-size: 30px;
	background: #000;
	overflow:hidden;
}
.footer {
	height: 250px;
	width: 100%;
	position: fixed;
	z-index: -1;
	bottom: 0px;
	left: 0px;
	background: url("<%= asset_path("footer.jpg") %>") no-repeat right 35px #000;
}
.slideshow img.active {
	opacity: 1;
}
.slides {
	height: 350px;	
}
.slides, .slideshow img {
	position: absolute;
	top: 0px;
	left: 0px;
	width: 100%;
}
.slideshow img {
	opacity: 0;	
	height: auto;
	-webkit-transition: opacity 0.8s;
}
.slideshow img:first-child {
	top: -50px;	
}
.navigation {
	position: absolute;
	bottom: 0px;
	width: 100%;
	font-size: 18px;
	color: #fff;
	padding: 25px 0;
	z-index: 4;
}
.navigation i {
	margin: 0 5px;
	text-shadow: 0 0 2px #000;
	display: none;
}
.div {
	position: absolute;
	bottom: 300px;
	width: 100%;
	color: #fff;
	padding: 25px 0;
	z-index: 4;
}
.div a {
	margin: 0 5px;
	text-shadow: 0 0 2px #000;
	border: 2px solid;
    padding: 10px;
    cursor: pointer;
}

