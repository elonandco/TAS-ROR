/* Body */
html, body {
  margin: 0;
  padding: 0;
  height: 100%;
  width: 100%;  
  -webkit-font-smoothing: antialiased;
}
/* Header */
.fixed .header,
.fixed .section-header {
  // position: fixed; 
}

.header {
	// width: 100%;
	// background: #202326;
	// height: 70px;
	// top: 0px;
	// left: 0px;
	// z-index: 2;
	// font-family: 'Lato', sans-serif;
	width: 78%;
    background: #fff;
    position: absolute;
    height: 70px;
    top: 3%;
    left: 10%;
    z-index: 2;
    font-family: 'Lato', sans-serif;
    -webkit-box-shadow: 0px 0px 42px -7px rgba(0,0,0,0.63);
  -moz-box-shadow: 0px 0px 42px -7px rgba(0,0,0,0.63);
  box-shadow: 0px 0px 42px -7px rgba(0,0,0,0.63);
}
.caption-learn {
  display: block;
  text-align: center;
  margin-top: 20px;
}
.learn-more {
  display: inline-block;
  border: 2px solid;
  width: 10%;
  font-size: 14px;
    font-weight: bolder;
    padding: 10px 40px;
}
.caption {
  z-index: 3;
  position: absolute;
  width: 100%;
  text-align: center;
  color: rgb(255, 255, 255);
  font-family: Raleway, Arial, sans-serif;
  text-transform: uppercase;
  font-size: 24px;
  background: -webkit-linear-gradient(rgba(0, 0, 0, 0), rgba(47,44,44,1));
  top: 130px;
  left: 0px;
  right: 0px;
  bottom: 0;
  padding: 45px 0 0 0;
  height: 450px;
}
.sponsored {
  color: #fff;
  text-align: left;
  font-weight: normal;
  font-size: 12px;
}
.caption span {
  display: block;
}
.caption .headline {
  font-weight: bold;
  font-size: 55px;
}
.caption .description {
  font-size: 30px;
  text-transform: none;
}
.search {
  overflow: hidden;
  height: 70px;
  width: 20%;
}
.search.clicked {
  overflow: visible;
  background: #f1f1f1;
  z-index: 3;
}
.content-box.videos-template .player {
  visibility: hidden;
  background: #000;
}
.bookmarks .content-box.videos-template.selected .player {
  visibility: visible;
}
.bookmarks .content-box.videos-template .player {
  position: absolute;
  z-index: 4;
  height: 100%;
  width: 100%;
  top: 0px;
}
.search.clicked .arrow-up {
  -ms-transform: rotate(180deg); /* IE 9 */
    -webkit-transform: rotate(180deg); /* Chrome, Safari, Opera */
    transform: rotate(180deg);  
}
.search.clicked .menu {
  opacity: 1;
  overflow:visible;
}
.search.clicked .selected-link .header-text{
  // color: #5b5b5b;
  color: #333;
}
.search.clicked .selected-link .arrow-up{
  border-bottom: 5px solid #202326;
}
.search.clicked .selected-link .currently-selected{
  color: #404040;
}

.logo {
  // background-image: url("<%= asset_path("TAS_logo.png") %>");
  background-image: url("<%= asset_path("white_logo.png") %>");
  height: 85px;
  // width: 271px;
  width: 192px;
    margin-left: 15px;
  position: fixed;
  background-size: 75% auto;
  background-position: center 9px;
  background-repeat: no-repeat;
  z-index: 3;
  top: 0px;
  -webkit-transition: -webkit-transform 0.2s ease-in-out;
  background-position-x: 15px;
}
.fixed .logo {
  -webkit-transform: scale(0.8);
  top: -8px;
  background-position: center -68px;
  background-position-x: 6px;;
}

.header .category, 
.header .date, 
.header .type,
.header .search-box {
  // color: #fff;
  color: #333;
  text-align: left;
  float: left;
  // width: 160px;
  clear: none;
  padding: 0;
  margin: 0 0px;
  position: relative;
}
.search-box form{
  // width: 234px;
  padding: 25px 10px;
}
.search-box form div{
  display: inline;
}
.search-box {
  width: 40%;
}
.search-box {
  width: 40%;
}
.search-box .header-input {
  border: 0 solid;
  background: transparent;
  // color: #fff;
  color: #333;
  font-size: 14px;
  width: 120px;
  font-family: 'Source Sans Pro', sans-serif;
  font-weight: bold;
}
.search-box .header-input:focus {
  outline:none;
}
a.search-btn {
    background: #00b288;
    padding: 12px 34px;
    color: #fff;
    font-weight: 600;
  -webkit-box-shadow: 0px 0px 5px 0px rgba(0, 0, 0, 0.63);
    -moz-box-shadow: 0px 0px 5px 0px rgba(0, 0, 0, 0.63);
    box-shadow: 0px 0px 5px 0px rgba(0, 0, 0, 0.63);
}
.search-fields .search:hover{
  background: #f1f1f1;
}
.search-fields .search:hover .header-text{
  color: #5b5b5b;
}
.search-fields .search:hover .arrow-up{
  border-bottom: 5px solid #202326;
}
.search-fields .search:hover .currently-selected{
  color: #404040;
}

.currently-selected {
  display: block;
  text-align: left;
  padding: 0 0 0 20px;
  text-transform: uppercase;
  color: #333;
  font-family: 'Source Sans Pro', sans-serif;
  font-size: 13px;
}

.header-text {
  color: #333;
  display: block;
  font-size: 11px;
  text-align: left;
  padding: 15px 0 5px 20px;
}

