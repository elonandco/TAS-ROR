}
.load-more.loaded:hover {
  color: #aaaaaa;
  background: #202326;
}
.bookmarks .content-box.ads-template {
  line-height: normal;
  background: #fff;
}
.content-box.ads-template span {
  display: block;
  text-align: center;
  -webkit-transform: translate(-50%,-50%);
  top: 50%;
  left: 50%;
  position: absolute;
}

.arrow {
  color: #fff;
  font-size: 85px;
}

.selected-link {
  height: 70px;
  text-align: center;
  font-family: 'Source Sans Pro', sans-serif;
  text-transform: none;
  font-weight: bold;
  font-size: 16px;
  color: #fff;
  // border-left: 1px solid #3e4145;
  border-right: 1px solid #3e4145;
  border-bottom: 5px solid #45ad58;
}

.arrow-up {
  width: 0;
  border-left: 5px solid transparent;
  border-right: 5px solid transparent;
  border-bottom: 5px solid #FFFFFF;
  position: absolute;
  right: 15px;
  top: 43px;
  display: none;
}
.caret {
  position: relative;
  left: -13%;
  font-size: 21px;
  line-height: 0;
  display: block;
  text-align: center;
  margin-top: -14px;
  height: 14px;
  color: #fff;
  .fa {
    text-shadow: 0px -3px 0px #ccc;
  }
}
.thumbnails {
  display: none;
  margin: 0 0 0 -10px;
  overflow: hidden;
}
.thumbnails li {
  width: 100px;
  height: 100px;
  float: left;
  margin: 5px;
  overflow: hidden;
  cursor: pointer;
  list-style: none;
  position: relative;
  border: solid 3px #000;
  background: #000;
}
.thumbnails li img {
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%,-50%);
  max-width: 100%;
  max-height: 100%;
  position: absolute;
}
.menu {
  background: #000;
  color: #fff;
  border-top: solid 1px #fff;
  font-weight: normal;
  opacity: 0;
  margin-top: 12px;
  -webkit-transition: opacity 0.2s ease-in-out;
}

.menu li {
  list-style: none;
  margin: 9px 20px 0 20px;
  font-size: 14px;
  text-align: left;
  text-transform: none;
    padding: 0 0 9px 0;
    color: #000;
}

.menu li.selected,.menu li:hover {
  list-style-type: none;
  color: #45ad58;
}

.menu li:last-child {
  border: none; 
  padding: 0 0 10px 0;
}

.search-fields {
	width: auto;
	cursor: pointer;
