.active-slider-date {
	background: #cf30e6;
    color: #fff;
}
.slider-footer {
	width: 100%;
	position: absolute;
    display: flex;
    top: 625px;
    background: #FFF;
    z-index: 3;
    color: #acacac;
    font-size: 18px;
    overflow: hidden;
}
.slider-date1 {
	width: 25%;
	padding-top: 15px;
    padding-bottom: 15px;
}
.slider-date2 {
	width: 25%;
	padding-top: 15px;
